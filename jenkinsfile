pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/lohithasree/jgsu-spring-petclinic.git'

                // Run Maven on a Unix agent.
               // sh "mvn -Dmaven.test.failure.ignore=true clean package"

                // To run Maven on a Windows agent, use
                 //bat "mvn -Dmaven.test.failure.ignore=true clean compile test package"
                 bat './mvnw clean compile package'
            }
            post {
                success {
                    junit '**/target/surefire-reports/TEST-*.xml'
                    archiveArtifacts 'target/*.jar'
                    
                    // for swnding emailnotifications
                //emailext attachLog: true, 
                //body: 'just go to url', 
                //compressLog: true, 
                //recipientProviders: [upstreamDevelopers(), 
                //requestor()], 
                //subject: 'job name and build number waiting for input'
             }
                }
            }
        }
    }

